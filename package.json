{
  "name": "revel-commit",
  "version": "1.2.0",
  "description": "Commitizen wrapper following the conventional-changelog format and asking for Shortcut story at Revel.",
  "bin": "./revel-cli.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hellorevel/revel-cz-conventional-changelog-for-shortcut.git"
  },
  "author": "Revel Engineering Team <engineering@hellorevel.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hellorevel/revel-commit"
  },
  "homepage": "https://github.com/hellorevel/revel-commit",
  "scripts": {
    "c": "yarn revel-commit"
  },
  "dependencies": {
    "@dionlarson/commitizen": "^0.0.0-semantically-released",
    "cz-conventional-changelog-for-shortcut": "^7.2.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog-for-shortcut"
    }
  },
  "devDependencies": {
    "revel-commit": "^1.2.0"
  }
}
